{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:42334",
      "sslPort": 44359
    }
  },
  "profiles": {
    "Smab.TTInfo.Server": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:7436;http://localhost:5354",
      "dotnetRunMessages": true
    },
    "Smab.TTInfo.Server 1hr cache": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "TTInfo__CacheHours": "1",
        "TTInfo__UseTestFiles": "false"
      },
      "applicationUrl": "https://localhost:7436;http://localhost:5354",
      "dotnetRunMessages": true
    },
	"IIS Express": {
		"commandName": "IISExpress",
		"launchBrowser": true,
		"environmentVariables": {
			"ASPNETCORE_ENVIRONMENT": "Development",
			"TTInfo__CacheHours": "1",
			"TTInfo__UseTestFiles": "false"
		}
	},
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "TTInfo__UseTestFiles": "true",
        "TTInfo__CacheFolder": "../cache",
        "TZ": "Europe/London"
      },
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}