@page "/maidenheadtest1"
@using Smab.TTInfo.Models.TTLeagues
@using System.Text.Json;
@inject TTLeaguesReader _ttleagues
@attribute [StreamRendering(true)]

<PageTitle>Maidenhead json test</PageTitle>

<pre>
	@JsonSerializer.Serialize(Fixtures, new JsonSerializerOptions { WriteIndented = true });
</pre>

@code {
	public required Fixtures Fixtures { get; set; }

	protected override async Task OnParametersSetAsync()
	{
		if (Fixtures is null) {
			Fixtures = await _ttleagues.GetAllFixtures("test", "test");
		}
	}

}
